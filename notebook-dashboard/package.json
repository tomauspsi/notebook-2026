    "@vitejs/plugin-react": "^5.0.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.0",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.35.0",
    "prettier": "^3.3.2"
  },
  "engines": {
    "node": ">=20.19.0"
  }
